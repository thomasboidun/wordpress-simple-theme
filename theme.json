{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"customTemplates": [],
	"settings": {
		"appearanceTools": true,
		"color": {
			"duotone": [],
			"gradients": [],
			"palette": []
		},
		"custom": {
			"spacing": {
				"none": "0",
				"x-small": "0.25rem",
				"small": "0.5rem"
			},
			"width": {
				"contentSize": "82.5rem",
				"wideSize": "100%"
			}
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "\"Open Sans\", serif",
					"name": "Open Sans",
					"slug": "open-sans",
					"fontFace": [
						{
							"fontFamily": "Open Sans",
							"fontWeight": "200 900",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [
								"file:./assets/fonts/open-sans/OpenSans-VariableFont_wdth,wght.ttf"
							]
						},
						{
							"fontFamily": "Open Sans",
							"fontWeight": "200 900",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": [
								"file:./assets/fonts/open-sans/OpenSans-Italic-VariableFont_wdth,wght.ttf"
							]
						}
					]
				}
			],
			"fontSizes": [
				{
					"name": "X-Small (8px)",
					"size": "0.5rem",
					"slug": "x-small"
				},
				{
					"name": "Small (12px)",
					"size": "0.75rem",
					"slug": "small"
				},
				{
					"name": "Default (16px)",
					"size": "1rem",
					"slug": "default"
				},
				{
					"name": "Medium (24px)",
					"size": "1.5rem",
					"slug": "medium"
				},
				{
					"name": "Large (32px)",
					"size": "2rem",
					"slug": "large"
				},
				{
					"name": "X-Large (48px)",
					"size": "3rem",
					"slug": "x-large"
				},
				{
					"name": "XX-Large (64px)",
					"size": "4rem",
					"slug": "xx-large"
				}
			]
		},
		"layout": {
			"contentSize": "var(--wp--custom--width--content-size)",
			"wideSize": "var(--wp--custom--width--wide-size)"
		}
	},
	"styles": {
		"blocks": {
			"core/columns": {
				"spacing": {
					"blockGap": "var(--wp--custom--spacing--small)",
					"margin": {
						"bottom": "var(--wp--custom--spacing--none)"
					},
					"padding": {
						"bottom": "var(--wp--custom--spacing--small)",
						"left": "var(--wp--custom--spacing--small)",
						"right": "var(--wp--custom--spacing--small)",
						"top": "var(--wp--custom--spacing--small)"
					}
				}
			},
			"core/cover": {
				"spacing": {
					"padding": {
						"bottom": "var(--wp--custom--spacing--small)",
						"left": "var(--wp--custom--spacing--small)",
						"right": "var(--wp--custom--spacing--small)",
						"top": "var(--wp--custom--spacing--small)"
					}
				}
			},
			"core/heading": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--custom--spacing--none)",
						"top": "var(--wp--custom--spacing--none)"
					}
				}
			},
			"core/paragraph": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--custom--spacing--none)",
						"top": "var(--wp--custom--spacing--none)"
					}
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--open-sans)",
			"fontSize": "var(--wp--preset--font-size--default)"
		},
		"spacing": {
			"blockGap": "var(--wp--custom--spacing--none)"
		}
	},
	"templateParts": []
}