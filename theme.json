{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"customTemplates": [],
	"settings": {
		"appearanceTools": true,
		"color": {
			"duotone": [],
			"gradients": [],
			"palette": []
		},
		"custom": {
			"spacing": {
				"none": "0",
				"x-small": "0.25rem",
				"small": "0.5rem",
				"normal": "1rem"
			},
			"width": {
				"contentSize": "82.5rem",
				"wideSize": "100%"
			}
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			],
			"blockGap": true
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "\"Open Sans\", serif",
					"name": "Open Sans",
					"slug": "open-sans",
					"fontFace": [
						{
							"fontFamily": "Open Sans",
							"fontWeight": "200 900",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [
								"file:./assets/fonts/open-sans/OpenSans-VariableFont_wdth,wght.ttf"
							]
						},
						{
							"fontFamily": "Open Sans",
							"fontWeight": "200 900",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": [
								"file:./assets/fonts/open-sans/OpenSans-Italic-VariableFont_wdth,wght.ttf"
							]
						}
					]
				}
			],
			"fontSizes": [
				{
					"name": "X-Small (8px)",
					"size": "0.5rem",
					"slug": "x-small"
				},
				{
					"name": "Small (12px)",
					"size": "0.75rem",
					"slug": "small"
				},
				{
					"name": "Normal (16px)",
					"size": "1rem",
					"slug": "normal"
				},
				{
					"name": "Medium (20px)",
					"size": "1.25rem",
					"slug": "medium"
				},
				{
					"name": "Large (+24px)",
					"size": "clamp(1.5rem, calc(1.275rem + .3vw), calc(1.275rem + .3vw))",
					"slug": "large"
				},
				{
					"name": "X-Large (+28px)",
					"size": "clamp(1.75rem, calc(1.3rem + .6vw), calc(1.3rem + .6vw))",
					"slug": "x-large"
				},
				{
					"name": "XX-Large (+32px)",
					"size": "clam(2rem, calc(1.325rem + .9vw), calc(1.325rem + .9vw))",
					"slug": "xx-large"
				},
				{
					"name": "Huge (+40px)",
					"size": "clamp(2.5rem, calc(1.375rem + 1.5vw), calc(1.375rem + 1.5vw))",
					"slug": "huge"
				}
			]
		},
		"layout": {
			"contentSize": "var(--wp--custom--width--content-size)",
			"wideSize": "var(--wp--custom--width--wide-size)"
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"spacing": {
					"blockGap": "var(--wp--custom--spacing--normal)"
				}
			},
			"core/buttons": {
				"spacing": {
					"blockGap": "var(--wp--custom--spacing--normal)"
				}
			},
			"core/columns": {
				"spacing": {
					"blockGap": "var(--wp--custom--spacing--normal)",
					"margin": {
						"bottom": "var(--wp--custom--spacing--none)"
					},
					"padding": {
						"bottom": "var(--wp--custom--spacing--none)",
						"left": "var(--wp--custom--spacing--none)",
						"right": "var(--wp--custom--spacing--none)",
						"top": "var(--wp--custom--spacing--none)"
					}
				}
			},
			"core/cover": {
				"spacing": {
					"padding": {
						"bottom": "var(--wp--custom--spacing--none)",
						"left": "var(--wp--custom--spacing--none)",
						"right": "var(--wp--custom--spacing--none)",
						"top": "var(--wp--custom--spacing--none)"
					}
				}
			},
			"core/group": {
				"spacing": {
					"padding": {
						"bottom": "var(--wp--custom--spacing--none)",
						"left": "var(--wp--custom--spacing--none)",
						"right": "var(--wp--custom--spacing--none)",
						"top": "var(--wp--custom--spacing--none)"
					}
				}
			},
			"core/heading": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--custom--spacing--none)",
						"top": "var(--wp--custom--spacing--none)"
					}
				}
			},
			"core/paragraph": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--custom--spacing--none)",
						"top": "var(--wp--custom--spacing--none)"
					}
				}
			},
			"core/separator": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--custom--spacing--none)",
						"top": "var(--wp--custom--spacing--none)"
					}
				}
			}
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--huge)"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--open-sans)",
			"fontSize": "var(--wp--preset--font-size--normal)"
		},
		"spacing": {
			"blockGap": "var(--wp--custom--spacing--none)"
		}
	},
	"templateParts": []
}